vector<int> compareTriplets(vector<int> a, vector<int> b) {
    vector<int> ans;
    int alice=0;
    int bob=0;
    if(a[0]<b[0])   bob++;
    else if(a[0]>b[0])  alice++;
    
    if(a[1]<b[1])   bob++;
    else if(a[1]>b[1])    alice++;
    
    if(a[2]<b[2])   bob++;
    else if(a[2]>b[2])    alice++;
    
    ans.push_back(alice);
    ans.push_back(bob);
    
    return ans;
}
